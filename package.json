{
  "name": "redis-orm-lite",
  "version": "1.0.3",
  "description": "A lightweight Redis ORM with a Mongoose-like API (find, findOne, updateMany, skip, limit, sort, etc.)",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "prepublishOnly": "npm run build && npm run build:cjs",
    "start": "node dist/index.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "redis",
    "orm",
    "mongoose",
    "typescript",
    "nosql",
    "database"
  ],
  "author": "Fatjon Mileti",
  "license": "MIT",
  "dependencies": {
    "redis": "^4.6.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "typescript": "^5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FatjonMileti/redis-orm-lite.git"
  },
  "bugs": {
    "url": "https://github.com/FatjonMileti/redis-orm-lite/issues"
  },
  "homepage": "https://github.com/FatjonMileti/redis-orm-lite#readme",

  "publishConfig": {
  "access": "public"
}

}
