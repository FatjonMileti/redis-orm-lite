{
  "name": "redis-orm-lite",
  "version": "1.0.8",
  "description": "A lightweight Redis ORM with a Mongoose-like API (find, findOne, updateMany, skip, limit, sort, etc.)",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js", 
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
  "build": "tsc && tsc --project tsconfig.cjs.json",
  "prepublishOnly": "npm run build",
  "start": "node dist/index.js"
},
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "redis",
    "orm",
    "mongoose",
    "typescript",
    "nosql",
    "database"
  ],
  "author": "Fatjon Mileti",
  "license": "MIT",
  "dependencies": {
    "ioredis": "^5.7.0",
    "redis": "^4.6.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FatjonMileti/redis-orm-lite.git"
  },
  "bugs": {
    "url": "https://github.com/FatjonMileti/redis-orm-lite/issues"
  },
  "homepage": "https://github.com/FatjonMileti/redis-orm-lite#readme",
  "publishConfig": {
    "access": "public"
  }
}
